/**
 * ROFI Color theme
 * User: Qball
 * Copyright: Dave Davenport
 */

window {
    anchor:     south;
    location:   south;
    width:      20%;
    padding:    9px;
    children:   [ horibox ];
}
horibox {
    orientation: horizontal;
    children:   [ prompt, entry, listview];
    spacing: 5px;
}
listview {
    layout:     horizontal;
    spacing:    5px;
    lines:      1;
}
entry {
    expand:     false;
    placeholder: "Applications";
    width:      10em;
}
element {
    padding: 0px 2px;
}
element selected {
    background-color: SteelBlue;
}

element-text, element-icon {
  background-color: inherit;
  text-color: inherit;
}
